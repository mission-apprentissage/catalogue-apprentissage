{
  "name": "mna-catalogue-apprentissage-server",
  "version": "5.19.0",
  "description": "[MNA] App catalogue apprentissage",
  "repository": "https://github.com/mission-apprentissage/catalogue-apprentissage.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/**/*.test.js",
    "test:coverage": "nyc --reporter=lcov mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/**/*.test.js",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:start-ci": "docker-compose up -d mongodb",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "jobs": "node src/jobs/index.js",
    "rcoFormation:duplicate": "standalone=true node src/jobs/oneshot/findRcoDuplicates/index.js",
    "uai:check": "standalone=true node src/jobs/checkUai/index.js",
    "compute:distance": "standalone=true node src/jobs/oneshot/computeDistance/index.js",
    "etablissements:tags": "standalone=true node src/jobs/etablissements/tags.js",
    "etablissements:jobs": "standaloneJobs=true node src/jobs/etablissements/index.js",
    "formations:jobs": "standaloneJobs=true node src/jobs/formations/index.js",
    "qualiopi": "node src/jobs/oneshot/qualiopi/index.js",
    "es:index": "standalone=true node src/jobs/esIndex/index.js",
    "doc": "node src/jobs/doc/index.js",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "tco:jobs": "node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/index.js",
    "bcn:jobs": "run=true node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/bcnDownloader/index.js; run=true node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/bcnImporter/index.js ",
    "onisep:jobs": "run=true node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/onisepImporter/index.js",
    "conventionFiles:jobs": "run=true node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/conventionFilesImporter/index.js",
    "rncp:jobs": "run=true node node_modules/@mission-apprentissage/tco-service-node/lib/jobs/rncpImporter/index.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.1",
    "@mission-apprentissage/tco-service-node": "2.42.5",
    "aws-sdk": "2.1109.0",
    "axios": "0.21.4",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.8.2",
    "command-line-args": "5.2.0",
    "config": "3.3.1",
    "connect-mongo": "4.4.1",
    "convert-csv-to-json": "1.3.3",
    "csv-parse": "5.3.0",
    "deep-object-diff": "^1.1.0",
    "ejs": "3.1.7",
    "express": "4.17.1",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "6.3.0",
    "express-session": "1.17.1",
    "fs": "0.0.1-security",
    "fs-extra": "9.0.0",
    "full-icu": "1.3.1",
    "joi": "17.2.1",
    "json-schema-to-typescript": "10.1.5",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "luxon": "2.3.1",
    "migrate-mongo": "8.2.3",
    "mjml": "4.12.0",
    "mongoose": "5.13.14",
    "mongoose-paginate": "5.0.3",
    "mongoose-schema-jsonschema": "1.4.7",
    "multer": "1.4.2",
    "node-stream-zip": "1.15.0",
    "nodemailer": "^6.4.14",
    "nodemailer-html-to-text": "^3.1.0",
    "oleoduc": "0.7.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "prettier": "2.0.5",
    "require-all": "3.0.0",
    "sanitize-filename": "1.6.3",
    "sha512crypt-node": "0.1.0",
    "stream-json": "1.7.4",
    "swagger-jsdoc": "6.2.0",
    "swagger-ui-express": "4.3.0",
    "xlsx": "0.18.5"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.10.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "faker": "5.5.3",
    "husky": "4.3.8",
    "lint-staged": ">=10",
    "mocha": "9.2.2",
    "nodemon": "2.0.15",
    "nyc": "15.1.0",
    "rewiremock": "3.14.3",
    "sinon": "11.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged && yarn doc"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
