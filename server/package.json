{
  "name": "mna-catalogue-apprentissage-server",
  "version": "6.10.0",
  "description": "[MNA] App catalogue apprentissage",
  "repository": "https://github.com/mission-apprentissage/catalogue-apprentissage.git",
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/**/*.test.js",
    "test:coverage": "nyc --reporter=lcov mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/**/*.test.js",
    "lint": "prettier --write 'src/**/*.js' 'tests/**/*.js'",
    "docker:start": "docker compose up -d --build --force-recreate",
    "docker:start-ci": "docker compose up -d mongodb",
    "docker:stop": "docker compose stop",
    "docker:down": "docker compose down",
    "docker:clean": "docker compose kill && docker system prune --force --volumes",
    "jobs": "node src/jobs/index.js",
    "startCampagne": "node src/jobs/startCampagne.js",
    "psup:prepare": "standalone=true node src/jobs/parcoursup/prepare.js",
    "psup:import": "standalone=true node src/jobs/parcoursup/importer.js",
    "psup:report": "standalone=true node src/jobs/parcoursup/reportRejected.js",
    "psup:export": "standalone=true node src/jobs/parcoursup/export/index.js",
    "uai:check": "standalone=true node src/jobs/checkUai/index.js",
    "compute:distance": "standalone=true node src/jobs/oneshot/computeDistance/index.js",
    "bcn:jobs": "standaloneJobs=true node src/jobs/bcn/index.js ",
    "etablissements:jobs": "standaloneJobs=true node src/jobs/etablissements/index.js",
    "formations:jobs": "standaloneJobs=true node src/jobs/formations/index.js",
    "affelnet:jobs": "standaloneJobs=true node src/jobs/affelnet/index.js",
    "affelnet:stats": "standalone=true node src/jobs/affelnet/stats.js",
    "psup:jobs": "standaloneJobs=true node src/jobs/parcoursup/index.js",
    "psup:stats": "standalone=true node src/jobs/parcoursup/stats.js",
    "qualiopi": "node src/jobs/oneshot/qualiopi/index.js",
    "es:index": "standalone=true node src/jobs/esIndex/index.js",
    "doc": "node src/jobs/doc/index.js",
    "migration:create": "migrate-mongo create",
    "migration:status": "migrate-mongo status",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "prepare": "cd .. && husky install server/.husky"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.15.0",
    "@mechanicalhuman/bunyan-pretty": "3.1.0",
    "axios": "1.11.0",
    "body-parser": "1.20.3",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-mongoose-stream": "1.0.2",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "1.0.0",
    "cli-progress": "3.12.0",
    "command-line-args": "6.0.0",
    "compression": "1.7.4",
    "config": "3.3.12",
    "connect-mongo": "6.0.0",
    "convert-csv-to-json": "2.49.0",
    "cors": "2.8.5",
    "csv-parse": "5.5.6",
    "deep-object-diff": "^1.1.9",
    "ejs": "3.1.10",
    "express": "4.21.2",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "7.4.0",
    "express-session": "1.18.0",
    "fs": "0.0.1-security",
    "fs-extra": "11.1.0",
    "full-icu": "1.5.0",
    "generate-password": "^1.7.1",
    "joi": "17.13.3",
    "json-schema-to-typescript": "15.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "migrate-mongo": "11.0.0",
    "mjml": "4.15.3",
    "mongodb": "6.0.0",
    "mongoose": "7.8.7",
    "mongoose-diff-history": "2.1.0",
    "mongoose-paginate": "5.0.3",
    "mongoose-schema-jsonschema": "2.1.1",
    "mongoose-to-swagger": "1.5.1",
    "multer": "1.4.2",
    "node-stream-zip": "1.15.0",
    "nodemailer": "^6.9.15",
    "nodemailer-html-to-text": "^3.1.0",
    "objects-to-csv": "1.3.6",
    "oleoduc": "0.8.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "query-string": "6",
    "rate-limiter-flexible": "2.2.1",
    "require-all": "3.0.0",
    "sanitize-filename": "1.6.3",
    "sha512crypt-node": "1.0.2",
    "stream-json": "1.7.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "title-case": "3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "axiosist": "1.2.0",
    "chai": "6.2.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "faker": "5.5.3",
    "husky": "8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "10.2.0",
    "nodemon": "2.0.15",
    "nyc": "17.1.0",
    "prettier": "3.6.2",
    "sinon": "^19"
  },
  "mocha": {},
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20",
    "yarn": ">=4.5.0"
  },
  "packageManager": "yarn@4.5.0+sha512.837566d24eec14ec0f5f1411adb544e892b3454255e61fdef8fd05f3429480102806bac7446bc9daff3896b01ae4b62d00096c7e989f1596f2af10b927532f39"
}
