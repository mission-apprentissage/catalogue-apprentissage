{
  "name": "mna-catalogue-apprentissage-server",
  "version": "4.8.0",
  "description": "[MNA] App catalogue apprentissage",
  "repository": "https://github.com/mission-apprentissage/catalogue-apprentissage.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/jobs/**/tests/",
    "test:coverage": "nyc --reporter=lcov mocha --file tests/globalSetup.js --recursive --exit --timeout 60000 tests/ src/jobs/**/tests/",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:start-ci": "docker-compose up -d mongodb",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed": "node src/jobs/seed/index.js",
    "jobs": "node src/jobs/index.js",
    "rco:importer": "standalone=true node src/jobs/rcoImporter/index.js",
    "rco:converter": "standalone=true node src/jobs/rcoConverter/index.js",
    "trainings:updater": "standalone=true node src/jobs/trainingsUpdater/index.js",
    "psup:prepare-2021": "standalone=true node src/jobs/parcoursup/prepare.js",
    "psup:import-2021": "standalone=true node src/jobs/parcoursup/importer.js",
    "psup:coverage-2021": "standalone=true node src/jobs/parcoursup/coverage.js",
    "psup:perimetre": "standalone=true node src/jobs/parcoursup/perimetre/index.js",
    "psup:updatemna": "standalone=true node src/jobs/parcoursup/updateMatchInfo.js",
    "psup:report": "standalone=true node src/jobs/parcoursup/reportRejected.js",
    "affelnet:coverage": "standalone=true node src/jobs/affelnet/coverage.js",
    "affelnet:reconciliation": "standalone=true node src/jobs/affelnet/reconciliation.js",
    "affelnet:reference": "standalone=true node src/jobs/affelnet/reference/index.js",
    "affelnet:perimetre": "standalone=true node src/jobs/affelnet/perimetre/index.js",
    "affelnet:import": "standalone=true node src/jobs/oneshot/afFormation/index.js",
    "affelnet:missing-cfd": "standalone=true node src/jobs/oneshot/afFormation/missingCfd.js",
    "es:index": "standalone=true node src/jobs/esIndex/index.js",
    "generate:swaggerschema": "standalone=true node src/jobs/generateSwaggerSchema/index.js",
    "etablissement:dump": "standalone=true node src/jobs/etablissements",
    "rcoPending:export": "standalone=true node src/jobs/pendingFormations/index.js",
    "rcoFormation:duplicate": "standalone=true node src/jobs/oneshot/findRcoDuplicates/index.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.1",
    "@mission-apprentissage/tco-service-node": "2.24.1",
    "aws-sdk": "2.785.0",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.8.2",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "connect-mongo": "4.4.1",
    "convert-csv-to-json-latin": "1.0.6",
    "deep-object-diff": "^1.1.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "elasticsearch-scroll-stream": "1.2.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "fs-extra": "9.0.0",
    "full-icu": "1.3.1",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "luxon": "^1.25.0",
    "match-sorter": "6.3.0",
    "mjml": "^4.7.1",
    "moment": "2.26.0",
    "mongoose": "5.13.7",
    "mongoose-paginate": "5.0.3",
    "mongoose-schema-jsonschema": "1.4.7",
    "multer": "1.4.2",
    "multipipe": "4.0.0",
    "nodemailer": "^6.4.14",
    "nodemailer-html-to-text": "^3.1.0",
    "oleoduc": "0.5.11",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "require-all": "3.0.0",
    "sha512crypt-node": "0.1.0",
    "string-similarity": "4.0.4",
    "swagger-jsdoc": "6.0.1",
    "swagger-ui-express": "4.1.6",
    "xlsx": "0.16.9"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": ">=4",
    "lint-staged": ">=10",
    "mocha": "9.1.0",
    "nodemon": "2.0.3",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "rewiremock": "3.14.3",
    "sinon": "11.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
